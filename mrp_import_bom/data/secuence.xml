<?xml version="1.0" encoding="utf-8"?>
<odoo>
<!--
se pone noupdate para que la secuancia no se encere cada vez que se
que se actualiza el módulo
-->
    <data noupdate="1">

        <!-- Sequences for sale.order -->
        <record id="seq_orden_reparacion" model="ir.sequence">
            <field name="name">Orden Reparación</field>
            <field name="code">orden.reparacion</field>
            <field name="prefix">OR</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>